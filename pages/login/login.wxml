<!--page/login/login.wxml-->
<view class="L_outer">
  <view class="L_logiImg">
    <image src=" "></image>
  </view>
  <view class="L_content">
    <view>
      <input type="number" />
    </view>

    <view>
      <input type="number" />
      <button class="getSecurityCode" type="default" loading="{{loading}}" disabled="{{disabled}}" bindtap="getSecurityCode" hover-class="other-button-hover"> 获取验证码</button>
    </view>

    <view>
      <button class="register" size="default" type="primary" loading="{{loading}}" disabled="{{disabled}}" bindtap="register">立即注册</button>
    </view>
  </view>
</view>