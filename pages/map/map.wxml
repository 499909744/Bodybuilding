<view class="map_text" wx:if="{{textShow}}">
  <text class="h1">{{textData.name}}</text>
  <text>{{textData.desc}}</text>
</view>
<view class="map_container">
  <map class="map" id="map" controls="{{controls}}" bindcontroltap="controltap" longitude="{{longitude}}" latitude="{{latitude}}" scale="12" show-location="true" markers="{{markers}}" bindmarkertap="makertap"></map>
</view>

<view class='buttom__btn'>
  <text>生成二维码</text>
</view>